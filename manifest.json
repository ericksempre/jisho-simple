{
  "name": "Jisho Minimal",
  "version": "0.1.0",
  "description": "Remove unnecessary elements from jisho.org website",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://jisho.org/*"
  ],
  "action": {
    "default_title": "Jisho Minimal"
  },
  "content_scripts": [{
    "js": ["content-script.js"],
    "matches": [
      "*://jisho.org/*"
    ]
  }],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 3
}